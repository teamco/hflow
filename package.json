{
  "private": true,
  "name": "hflow",
  "version": "0.0.5",
  "scripts": {
    "start:linux": "cross-env PORT=8003 max dev",
    "start:linux:proxy": "cross-env CORPORATE_PROXY=true PORT=8003 max dev",
    "analize": "cross-env ANALYZE=1 max build",
    "wait:port": "wait-port 8003",
    "umi:envs": "cross-env NODE_ENV=development node -r esm ./config/envs.js",
    "umi:routes": "cross-env NODE_ENV=development node -r esm ./config/routes.js",
    "umi:proxy": "cross-env NODE_ENV=development node -r esm ./config/proxy.js",
    "umi:alias": "cross-env NODE_ENV=development node -r esm ./config/alias.js",
    "umi:models": "max dva list model",
    "umi:clean": "node ./preinstall.js",
    "version:update": "yarn set version stable",
    "libs:rebuild": "yarn rebuild",
    "libs:update": "yarn upgrade-interactive",
    "umi:lint": "max lint",
    "umi:lint:fix": "max lint --fix",
    "build:verification": "serve ./dist",
    "build": "max build",
    "deploy": "yarn run build",
    "postdeploy": "now",
    "precommit": "lint-staged",
    "preinstall": "yarn umi:clean",
    "postinstall": "max setup",
    "js2flowchart": "js2flowchart ./src/pages/landing/landing.js",
    "e2e:chrome": "yarn playwright test --project=chromium",
    "e2e:chrome:debug": "DEBUG=pw:browser yarn playwright test --project=chromium",
    "e2e:report": "yarn playwright show-report __report__/e2e/current/html",
    "unit:test": "cross-env TS_NODE_TRANSPILE_ONLY=yes jest --json --outputFile=./__report__/result.json --testLocationInResults --passWithNoTests",
    "unit:test:coverage": "cross-env TS_NODE_TRANSPILE_ONLY=yes jest --passWithNoTests --coverage --json --testLocationInResults --outputFile=./__report__/coverage.json",
    "single:suite": "cross-env TS_NODE_TRANSPILE_ONLY=yes jest --passWithNoTests ./src/components/Footer/__tests__/footer.test.js",
    "single:profile": "cross-env TS_NODE_TRANSPILE_ONLY=yes jest --passWithNoTests ./src/pages/landing/profile/__tests__/profile.spec.js",
    "single:components.Address": "cross-env TS_NODE_TRANSPILE_ONLY=yes jest --no-cache --passWithNoTests ./src/components/Address/__tests__/address.spec.js",
    "single:utils.dom": "cross-env TS_NODE_TRANSPILE_ONLY=yes jest --passWithNoTests ./src/utils/__tests__/dom.test.js",
    "single:test": "cross-env TS_NODE_TRANSPILE_ONLY=yes jest --passWithNoTests ./src/components/Common/Tags/__tests__/common.tags.test.js -t 'Hooked tags'"
  },
  "bugs": {
    "url": "https://github.com/get-me-home/frontend/issues",
    "email": "teamco@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/get-me-home/frontend.git"
  },
  "lint-staged": {
    "*.{js,jsx,css,less}": [
      "max lint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.5",
    "@ant-design/plots": "^1.2.5",
    "@casl/ability": "^6.5.0",
    "@casl/react": "^3.1.0",
    "@cloudinary/react": "^1.11.2",
    "@cloudinary/url-gen": "^1.11.2",
    "@react-awesome-query-builder/antd": "^6.4.1",
    "antd": "^5.8.4",
    "antd-img-crop": "^4.12.2",
    "babel-plugin-formatjs": "^10.5.3",
    "capitalize-first-letter": "^1.0.0",
    "classnames": "^2.3.2",
    "countries-and-timezones": "^3.5.1",
    "country-codes-list": "^1.6.11",
    "country-flag-icons": "^1.5.7",
    "dva-model-extend": "^0.1.2",
    "file-saver": "^2.0.5",
    "firebase": "^10.2.0",
    "generate-password": "^1.7.0",
    "gravatar": "^1.8.2",
    "html-to-image": "^1.11.11",
    "lodash": "^4.17.21",
    "mockjs": "^1.1.0",
    "pluralize": "^8.0.0",
    "provinces": "^1.11.0",
    "query-string": "^8.1.0",
    "re-resizable": "^6.9.11",
    "react-google-maps": "^9.4.5",
    "react-html-parser": "^2.0.2",
    "react-interval": "^2.1.2",
    "react-json-to-csv": "^1.2.0",
    "react-json-view": "^1.21.3",
    "react-resizable": "^3.0.5",
    "react-use-scroll-indicator": "^1.1.1",
    "short-uuid": "^4.2.2",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-export-default-from": "^7.22.5",
    "@babel/plugin-transform-private-methods": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-typescript": "^7.22.5",
    "@playwright/test": "^1.37.1",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^6.0.1",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^18.17.8",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@umijs/babel-preset-umi": "^4.0.76",
    "@umijs/max": "^4.0.76",
    "@umijs/plugin-request": "^2.9.0",
    "@umijs/test": "^4.0.76",
    "babel-jest": "^29.6.3",
    "click-to-react-component": "^1.0.8",
    "colors": "^1.4.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "enzyme": "^3.11.0",
    "eslint": "^8.47.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "esm": "^3.2.25",
    "jest": "^29.6.3",
    "jest-environment-jsdom": "^29.6.3",
    "jest-html-reporters": "^3.1.4",
    "jest-junit": "^16.0.0",
    "jest-transform-stub": "^2.0.0",
    "js2flowchart": "^1.3.4",
    "lint-staged": "^14.0.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "redux-logger": "^3.0.6",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "yorkie": "^2.0.0"
  },
  "packageManager": "yarn@3.6.2",
  "engines": {
    "node": "^16.x"
  }
}
