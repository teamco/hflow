{
  "private": true,
  "name": "hflow",
  "version": "0.0.1",
  "scripts": {
    "start:linux": "cross-env PORT=8003 umi dev",
    "analize": "cross-env ANALYZE=1 umi build",
    "start:windows": "cross-env NODE_ENV=development NODE_OPTIONS=--openssl-legacy-provider PORT=8003 umi dev",
    "start:windows:node16": "cross-env NODE_ENV=development PORT=8003 umi dev",
    "model:list": "umi dva list model",
    "umi:clean": "node ./preinstall.js",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "test": "umi-test",
    "single:suite": "umi-test ./src/components/Common/Tags/__tests__/common.tags.test.js",
    "single:test": "umi-test ./src/components/Common/Tags/__tests__/common.tags.test.js -t 'Hooked tags'",
    "test:coverage": "umi-test --coverage",
    "cypress:int": "cypress open --config integrationFolder=./cypress/integration",
    "cypress:int:ci": "cypress run --config integrationFolder=./cypress/integration"
  },
